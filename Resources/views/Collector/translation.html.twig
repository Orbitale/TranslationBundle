{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <img width="23" height="28" alt="Translations" src="data:image/png;base64," />
    <span class="sf-toolbar-status">{{ collector.count }}</span>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>Used</b>
        <span>{{ collector.count }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Untranslated</b>
        <span>{{ collector.untranslated }}</span>
    </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
    <span class="icon"><img src="data:image/png;base64," alt="Configuration" /></span>
    <strong>Translations</strong>
    <span class="count">
        <span>{{ collector.count }}</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    <h2>Translations</h2>

    {% if not collector.tokens %}
        <p>
            <em>No translations used.</em>
        </p>
    {% else %}
        {% render url('orbitale_profiler_translations', {'token': token}) %}
    {% endif %}
{% endblock %}
